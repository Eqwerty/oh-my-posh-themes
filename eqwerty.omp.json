{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#00cc00",
          "foreground": "#000",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#ff9900",
          "foreground": "#000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "full"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background_templates": ["{{ if or (.Working.Changed) (.Staging.Changed) (gt .StashCount 0) }}#ffc800{{ end }}"],
          "background": "#02cbd4",
          "foreground": "#000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true
          },
          "style": "powerline",
          "template": "{{ if .Detached }}{{ if .Rebase }} {{ trimPrefix \"\uE0A0\" .Rebase.HEAD }}|REBASE {{ .Rebase.Current }}/{{ .Rebase.Total }}{{ else }} {{ if .Commit.Refs.Remotes }}{{ first .Commit.Refs.Remotes }} {{ end }}({{ abbrev 13 .Commit.Sha }}){{ end }}{{ else }}{{ if .Merge }} {{ .Ref }}|MERGING{{ else }} {{ .Ref }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ end }}{{ end }}{{ if gt .StashCount 0 }} @{{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#ff1ac6",
          "foreground": "#000",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0b4",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": ">",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
